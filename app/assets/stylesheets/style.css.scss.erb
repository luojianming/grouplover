@charset "utf-8";
/* CSS Document */

body {
	padding-top: 80px;
	padding-bottom: 40px;
	//background-image: url(<%= asset_path 'paper.jpg' %>);
  background-image: url(<%= asset_path 'paper.jpg' %>);
  background-repeat: repeat;
}
.nav-menu {
	height: 30px;
	width: 100%;
	background-color:#FAF7F7;
	position: fixed;
	z-index:20;
	top: 40px;
	border-bottom-width: 1px;
	border-bottom-style: solid;
	border-bottom-color: #CCC;
}

.nav-menu ul{
	margin-bottom:20px;
	text-align: center;
}
.nav-menu ul li {
	display: inline;
	margin-right: 20;
	list-style-type: none;
}

.nav-menu ul li a .caret{
	margin-top:13px;
	margin-right:10px;
}

.hp-container {
	margin: auto;
	height: 1200px;
	
}

.hp-mainbody {
	height: 1000px;
	border: 4px solid #F00;
	margin-top: 10px;
	margin-right: auto;
	margin-bottom: auto;
	margin-left: auto;
}
.hp-mainbody-top {
	margin: auto;
	height: 80px;
}
.pull-right.latest_invitor_more, .pull-right.following_more{
	margin-bottom: 10px;
	position: relative;
	right: 10px;
}
.personalpage_left {
	margin-bottom: 30px;
	border-bottom-width: 1px;
	border-bottom-style: solid;
	border-top-color: #CCC;
	border-right-color: #FFF;
	border-bottom-color: #CCC;
	border-left-color: #FFF;
	border-top-width: 1px;
	border-top-style: solid;
}


.hero-unit-bg {
	background-color: #CCC;
}
.container footer {
	text-align: center;
}

.bs-docs-sidenav {
  width: 228px;
  margin: 30px 0 0;
  padding: 0;
  background-color: #fff;
  -webkit-border-radius: 6px;
     -moz-border-radius: 6px;
          border-radius: 6px;
  -webkit-box-shadow: 0 1px 4px rgba(0,0,0,.065);
     -moz-box-shadow: 0 1px 4px rgba(0,0,0,.065);
          box-shadow: 0 1px 4px rgba(0,0,0,.065);
}
.bs-docs-sidenav > li > a {
  display: block;
  *width: 190px;
  margin: 0 0 -1px;
  padding: 8px 14px;
  border: 1px solid #e5e5e5;
}
.bs-docs-sidenav > li:first-child > a {
  -webkit-border-radius: 6px 6px 0 0;
     -moz-border-radius: 6px 6px 0 0;
          border-radius: 6px 6px 0 0;
}
.bs-docs-sidenav > li:last-child > a {
  -webkit-border-radius: 0 0 6px 6px;
     -moz-border-radius: 0 0 6px 6px;
          border-radius: 0 0 6px 6px;
}
.bs-docs-sidenav > .active > a {
  position: relative;
  z-index: 2;
  padding: 9px 15px;
  border: 0;
  text-shadow: 0 1px 0 rgba(0,0,0,.15);
  -webkit-box-shadow: inset 1px 0 0 rgba(0,0,0,.1), inset -1px 0 0 rgba(0,0,0,.1);
     -moz-box-shadow: inset 1px 0 0 rgba(0,0,0,.1), inset -1px 0 0 rgba(0,0,0,.1);
          box-shadow: inset 1px 0 0 rgba(0,0,0,.1), inset -1px 0 0 rgba(0,0,0,.1);
}

.bs-docs-sidenav .icon-chevron-right {
  float: right;
  margin-top: 2px;
  margin-right: -6px;
  opacity: .25;
}
.bs-docs-sidenav > li > a:hover {
  background-color: #f5f5f5;
}
.bs-docs-sidenav a:hover .icon-chevron-right {
  opacity: .5;
}
.bs-docs-sidenav .active .icon-chevron-right,
.bs-docs-sidenav .active a:hover .icon-chevron-right {
 background-image: url( <%= asset_path 'glyphicons-halflings-white.png' %> );
  opacity: 1;
}
.bs-docs-sidenav.affix {
  top: 40px;
}
.bs-docs-sidenav.affix-bottom {
  position: absolute;
  top: auto;
  bottom: 270px;
}

.span3.bs-docs-sidebar {
	position: fixed;
	top: 80px;
}
.profile-style , .setting-style,.notification-style{
	padding-top: 30px;
}


.main {
  border-top: 1px solid #eee;
}
.main .sidenav{
	min-height: 400px;
    padding: 30px 0;
    margin: 0;
}
.main .sidenav ul {
    list-style: none;
    margin: 0;
    padding: 0 0 0 0; 
}

 .main .sidenav ul li {
        margin: 8px 0;
        padding: 0 20px;
        line-height: 28px;
        margin-right: 1px;
        border: 1px solid transparent;
        border-right: 0;
        border-left: 0; 
}

 .main .sidenav ul li a {
          color: #777;
          padding: 10px 10px;
          display: block;
          font-size: 16px;
          font-weight: normal; 
}

.main .sidenav ul li.active {
          border: 1px solid #CCC;
          border-right: 0;
          border-left: 0; 
}
.main .sidenav ul li a:hover {
          color: #111; 
		  text-decoration:none;
}
.main .sidenav ul li a:hover i.icon {
          opacity: 0.9; 
}
.main .sidenav ul li i.icon{
	 	  display: inline-block;
	 	  width: 20px;
     	  height: 20px;
          vertical-align: text-top;
          line-height: 24px;
          margin-right: 8px;
          opacity: 0.45;
		  -webkit-transition: 0.15s linear; 
}
